 $FCD1 LD DE,$A66C   ;
 $FCD4 LD BC,$0186   ;
 $FCD7 LDIR          ; }
; Remove a bunch of room exits (as actions in the game will open them
; back up).
 $FCD9 LD IX,$E978   ; #REGix=#R$E978.
 $FCDD LD A,($A7F0)  ; {#REGb=*#R$A7F0.
 $FCE0 LD B,A        ; }
