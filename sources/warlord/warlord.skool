@label=CheckItemInCurrentRoom
c$AF31 CALL $AED1    ; Call #R$AED1.
 $AF34 LD HL,$A7C3   ; {Compare the room ID with *#R$A7C3.
 $AF37 CP (HL)       ; }
 $AF38 RET           ; Return.

; Check Room Objects
;
; Used by the routine at #R$AC02.
;
;   A Room number
; O:A #N$00 or #N$01 if objects are found or not
@label=CheckRoomObjects
c$AF39 LD HL,$A66C   ; #REGhl=#R$A66C.
 $AF3C LD BC,($A7E6) ; #REGbc=*#R$A7E6.
@label=CheckRoomObjects_Loop
*$AF40 CPIR          ; Search for the room number in the table.
 $AF42 RET NZ        ; Return if no objects are found.
 $AF43 PUSH HL       ; Stash the object location table pointer on the stack.
 $AF44 CALL $AF56    ; Call #R$AF56.
 $AF47 POP HL        ; Restore the object location table pointer from the
                     ; stack.
 $AF48 JR NZ,$AF54   ; Jump to #R$AF54 if there are no objects found here.
 $AF4A LD E,A        ; Temporarily store #REGa in #REGe.
 $AF4B LD A,B        ; {Are there more objects to check?
 $AF4C OR C          ; }
 $AF4D LD A,E        ; Restore the #REGa register.
 $AF4E JR NZ,$AF40   ; Jump to #R$AF40 if there are more objects to check.
; Set the "successful" output.
 $AF50 LD A,$01      ; Set #REGa to #N$01 to indicate that objects were found.
 $AF52 AND A         ; Set flags.
 $AF53 RET           ; Return.
; Set the "failure" output.
@label=NoRoomObjectsFound
*$AF54 XOR A         ; Set #REGa to #N$00 to indicate that no objects were
                     ; found.
 $AF55 RET           ; Return.
