; Game Start
;
; Used by the routine at #R$B14A.
@label=GameStart
c$FCCE LD HL,$FD02   ; {Copy #N$0186 bytes from #R$FD02 to #R$A66C.
 $FCD1 LD DE,$A66C   ;
 $FCD4 LD BC,$0186   ;
 $FCD7 LDIR          ; }
; Remove a bunch of room exits (as actions in the game will open them
; back up).
 $FCD9 LD IX,$E978   ; #REGix=#R$E978.
 $FCDD LD A,($A7F0)  ; {#REGb=*#R$A7F0.
 $FCE0 LD B,A        ; }
 $FCE1 XOR A         ; Set #REGa to #N$00 which "removes" the exit.
@label=GameStart_Loop
*$FCE2 LD L,(IX+$00) ; #REGl=*#REGix+#N$00.
 $FCE5 LD H,(IX+$01) ; #REGh=*#REGix+#N$01.
 $FCE8 LD (HL),A     ; Write #REGa to *#REGhl.
 $FCE9 INC IX        ; {Move to the next address in the table.
 $FCEB INC IX        ; }
 $FCED DJNZ $FCE2    ; Decrease the exits counter by one and loop back to #R$FCE2 until
                     ; all the room exits have been cleared.
; Ensure three exits are "open" even though they point to the room you're
; in (they become real exits after picking up the staff in the game).
 $FCEF LD A,$50      ; {Write #N$50 to *#R$E8F4 to open up northbound access to #ROOM$50
 $FCF1 LD ($E8F4),A  ; from #ROOM$50.}
 $FCF4 LD ($E8F6),A  ; Write #N$50 to *#R$E8F6 to open up eastbound access to #ROOM$50
                     ; from #ROOM$50.
 $FCF7 LD ($E8F7),A  ; Write #N$50 to *#R$E8F7 to open up westbound access to #ROOM$50
                     ; from #ROOM$50.
 $FCFA LD A,$02      ; {Call #R$EB10 to room #N$02: #ROOM$02.
 $FCFC CALL $EB10    ; }
 $FCFF JP $E99F      ; Jump to #R$E99F.

