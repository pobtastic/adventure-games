; Add To Score
;
; Used by the routines at #R$EBDA, #R$EC07, #R$EC32, #R$EC40, #R$EC4E, #R$EC5C,
; #R$EC6A, #R$EC78, #R$EC86, #R$EC94, #R$ECBE, #R$EEE9, #R$EF5A, #R$EFAD,
; #R$F0DB, #R$F134, #R$F156, #R$F191, #R$F4B9, #R$F544, #R$F57B, #R$F5B8,
; #R$F739, #R$F793 and #R$F7FD.
;
; A Points to add
@label=AddToScore
c$B09A PUSH HL       ; Stash #REGhl on the stack.
 $B09B LD HL,($A7C4) ; Load *#R$A7C4 into #REGhl.
 $B09E ADD A,L       ; Add the points to the low byte.
 $B09F DAA           ; Decimal adjust for BCD (Binary Coded Decimal).
 $B0A0 LD L,A        ; Store the adjusted result.
 $B0A1 JR NC,$B0A4   ; Jump to #R$B0A4 if there was no carry from the addition.
; If there was carry, increment the high byte.
 $B0A3 INC H         ; Increment the high byte by one.
@label=SaveScore
*$B0A4 LD ($A7C4),HL ; Write the updated score back to *#R$A7C4.
 $B0A7 POP HL        ; Restore #REGhl from the stack.
 $B0A8 RET           ; Return.

; Print Scoring
;
; Used by the routines at #R$E9B2, #R$F84B and #R$F8A9.
; .
; Prints the players score as a percentage.
;
; .
;
; Print "#STR$AACE,$08($b==$FF)".
@label=Print_Scoring
c$B0A9 LD HL,$AACE   ; #REGhl=#R$AACE.
 $B0AC CALL $A585    ; Call #R$A585.
 $B0AF LD HL,($A7C4) ; #REGhl=*#R$A7C4.
; Evaluate the first byte to see if it needs printing at all.
; .
; So don't show "058%" - instead show "58%".
; .
; This particular check is for the first character "1" to see if the
; score is printing "100%".
 $B0B2 LD A,H        ; Take the first scoring byte and store it in #REGa.
 $B0B3 AND %00001111 ; {Keep only bits 0-3 and jump to #R$B0BC if the result of this is
 $B0B5 JR Z,$B0BC    ; zero.}
 $B0B7 ADD A,$30     ; {Convert the number to ASCII by adding #N$30 and call
 $B0B9 CALL $A5A4    ; #R$A5A4.}
@label=Scoring_SecondDigit
*$B0BC LD A,L        ; Load the second scoring byte into #REGa.
 $B0BD AND %11110000 ; {Keep only bits 4-7 and jump to #R$B0C6 if the result is
 $B0BF JR NZ,$B0C6   ; non-zero.}
; The "tens" digit is zero, so check the "hundreds" digit again to avoid
; printing "05%" - instead show only "5%".
 $B0C1 LD A,H        ; Load the first scoring byte into #REGa again.
 $B0C2 AND A         ; {Jump to #R$B0CF if no "hundreds" digit was printed.
 $B0C3 JR Z,$B0CF    ; }
; A "hundreds" digit was printed, so force a "0" to be printed.
 $B0C5 XOR A         ; Load #N$00 into #REGa for printing.
@label=Scoring_PrintTens
*$B0C6 RRCA          ; {Shift the bits four positions right. This moves the tens digit into
 $B0C7 RRCA          ; the lower part of the byte for printing.
 $B0C8 RRCA          ;
 $B0C9 RRCA          ; }
 $B0CA ADD A,$30     ; {Convert the number to ASCII by adding #N$30 and call
 $B0CC CALL $A5A4    ; #R$A5A4..}
; Lastly, always print the "units".
@label=Scoring_ThirdDigit
*$B0CF LD A,L        ; Load the second scoring byte into #REGa.
 $B0D0 AND %00001111 ; Keep only bits 0-3.
 $B0D2 ADD A,$30     ; {Convert the number to ASCII by adding #N$30 and call
 $B0D4 CALL $A5A4    ; #R$A5A4.}
; Print "#STR$AADF,$08($b==$FF)".
 $B0D7 LD HL,$AADF   ; #REGhl=#R$AADF.
 $B0DA CALL $A592    ; Call #R$A592.
 $B0DD RET           ; Return.

; Action Routing
;
; Used by the routines at #R$F853, #R$F8F2, #R$F956, #R$F975, #R$F9B3, #R$F9E2,
; #R$FA29, #R$FA6A, #R$FA91, #R$FAAD, #R$FADD, #R$FB21, #R$FB5D, #R$FB89,
; #R$FBC3, #R$FBDE, #R$FBF5, #R$FC10, #R$FC2F, #R$FC4B and #R$FC8D.
;
; HL Table of pointers which reference item groups
; DE Jump table of pointers which action each reference in #REGhl
; BC Count of the number of table items to process
@label=ActionRouting
c$B0DE PUSH HL       ; {Load the item group table into #REGix (using the
 $B0DF POP IX        ; stack).}
; Keep looping until a match is found.
@label=ActionRouting_Loop
*$B0E1 LD L,(IX+$00) ; {Fetch the phrase tokens from the item group and store the pointer
 $B0E4 LD H,(IX+$01) ; in #REGhl.}
 $B0E7 PUSH IX       ; {Stash the item group table, the pointer to the jump table and the
 $B0E9 PUSH DE       ; table items counter on the stack.
 $B0EA PUSH BC       ; }
 $B0EB CALL $AE80    ; Call #R$AE80.
 $B0EE POP BC        ; {Restore the table items counter, the pointer to the jump table and
 $B0EF POP DE        ; the item group table from the stack.
 $B0F0 POP IX        ; }
; Skip to the next set of phrase tokens if nothing matched...
 $B0F2 JR NZ,$B0FF   ; Jump to #R$B0FF if none of the input matches this set of phrase
                     ; tokens.
; The phrase tokens matched! Handle jumping to the action routine.
 $B0F4 PUSH DE       ; {Load the action table into #REGix (using the stack).
 $B0F5 POP IX        ; }
 $B0F7 LD L,(IX+$00) ; {Fetch the action routine from the table and store it in
 $B0FA LD H,(IX+$01) ; #REGhl.}
 $B0FD POP DE        ; Discard the return address on the stack.
 $B0FE JP (HL)       ; Jump to the action routine pointed to by *#REGhl.
; Nothing matched, so move to the next set of pointers.
@label=ActionRouting_Next
*$B0FF INC IX        ; {Move to the next pointer in the item group table.
 $B101 INC IX        ; }
 $B103 INC DE        ; {Move to the next pointer in the action pointers table.
 $B104 INC DE        ; }
 $B105 DEC BC        ; Decrease the table items counter by one.
 $B106 LD A,B        ; {Jump back to #R$B0E1 until all the item groups have
 $B107 OR C          ; been processed.
 $B108 JR NZ,$B0E1   ; }
 $B10A RET           ; Return.

; Print Room Image
;
; Used by the routine at #R$FF12.
;
; IX Pointer to room image data
@label=Print_RoomImage
c$B10B PUSH IX       ; {#REGhl=#REGix (using the stack).
 $B10D POP HL        ; }
 $B10E LD DE,$0200   ; {Add #N$0200 to the image data to skip the header.
 $B111 ADD HL,DE     ; }
 $B112 LD DE,$4000   ; Initialise the screen buffer pointer in #REGde to #N$4000 (the
                     ; beginning of the screen buffer).
 $B115 LD BC,$0200   ; Set a counter in #REGbc for #N$0200 bytes to process.
@label=DecompressPixelData_Loop
*$B118 PUSH BC       ; Stash the byte counter on the stack.
 $B119 LD B,$08      ; #REGb=#N$08.
 $B11B LD C,(IX+$00) ; #REGc=*#REGix+#N$00.
*$B11E SLA C         ; Shift #REGc left (with carry).
 $B120 JR NC,$B125   ; Jump to #R$B125 if ?? is greater than or equal to #N$08.
 $B122 LD A,(HL)     ; #REGa=*#REGhl.
 $B123 LD (DE),A     ; Write #REGa to *#REGde.
 $B124 INC HL        ; Increment #REGhl by one.
*$B125 INC DE        ; Increment #REGde by one.
 $B126 DJNZ $B11E    ; Decrease counter by one and loop back to #R$B11E until
                     ; counter is zero.
 $B128 INC IX        ; Increment #REGix by one.
 $B12A POP BC        ; Restore the byte counter from the stack.
 $B12B DEC BC        ; Decrease the byte counter by one.
 $B12C LD A,B        ; {Jump back to #R$B118 until all bytes have been
 $B12D OR C          ; processed.
 $B12E JR NZ,$B118   ; }
; Decompress the attribute data.
 $B130 PUSH HL       ; {#REGix=#REGhl (using the stack).
 $B131 POP IX        ; }
 $B133 LD HL,$5800   ; Initialise the attribute buffer pointer in #REGhl to #N$5800 (the
                     ; beginning of the attribute buffer).
@label=DecompressAttributeData_Loop
*$B136 LD A,(IX+$00) ; Fetch the attribute byte from *#REGix+#N$00 and store it
                     ; in #REGa.
 $B139 LD B,(IX+$01) ; Fetch the repeat count from *#REGix+#N$01 and store this
                     ; in #REGb.
 $B13C INC IX        ; {Increment #REGix by two.
 $B13E INC IX        ; }
@label=RepeatAttribute_Loop
*$B140 LD (HL),A     ; Write the attribute byte to the attribute buffer
                     ; pointer.
 $B141 INC HL        ; Increment the attribute buffer pointer by one.
 $B142 DJNZ $B140    ; Decrease the repeat counter by one and loop back to #R$B140 until
                     ; the counter is zero.
; Keep looping back until the end of the attribute buffer is reached.
 $B144 LD A,H        ; {Jump back to #R$B136 until #REGh is equal to #N$5A.
 $B145 CP $5A        ;
 $B147 JR NZ,$B136   ; }
 $B149 RET           ; Return.

