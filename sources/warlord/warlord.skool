; Validate Object
;
; Used by the routines at #R$AB97 and #R$AF39.
;
;   BC Object counter
; O:F Z is unset if the object is valid, unset when invalid
@label=ValidateObject
c$AF56 PUSH AF       ; {Stash #REGaf and #REGbc on the stack.
 $AF57 PUSH BC       ; }
 $AF58 INC BC        ; Adjust the object counter.
 $AF59 LD A,($A7E6)  ; #REGa=*#R$A7E6.
 $AF5C SUB C         ; {Calculate the index and store the result in #REGe.
 $AF5D LD E,A        ; }
 $AF5E LD IX,($A7CC) ; #REGix=*#R$A7CC.
 $AF62 CALL $AB88    ; Call #R$AB88.
 $AF65 POP BC        ; Restore the object counter from the stack.
 $AF66 LD A,H        ; {Jump to #R$AF6D if the object is invalid.
 $AF67 OR L          ;
 $AF68 JR Z,$AF6D    ; }
 $AF6A POP AF        ; Restore the object type from the stack.
 $AF6B AND A         ; Clear the Z flag.
 $AF6C RET           ; Return.
@label=InvalidObject
*$AF6D POP AF        ; Restore #REGaf from the stack.
 $AF6E CP A          ; Set the Z flag.
 $AF6F RET           ; Return.
