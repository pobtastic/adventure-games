; Is Character A Delimiter
;
; Used by the routine at #R$AD32.
;
; O:F The zero flag is set if a delimiter is found
@label=IsDelimiter
c$ACD8 PUSH HL       ; {Stash #REGhl and #REGbc on the stack.
 $ACD9 PUSH BC       ; }
 $ACDA LD HL,$A663   ; #REGhl=#R$A663.
 $ACDD LD C,(HL)     ; {Fetch the delimiter count and store it in #REGbc.
 $ACDE LD B,$00      ; }
 $ACE0 INC HL        ; Move #REGhl to point to the delimiter table.
 $ACE1 CPIR          ; Search for matching delimiters.
 $ACE3 POP BC        ; {Restore #REGbc and #REGhl from the stack.
 $ACE4 POP HL        ; }
 $ACE5 RET           ; Return.

; Print User Input To Screen
;
; Used by the routine at #R$AD32.
;
; A User input keypress
; HL Pointer to the command buffer
@label=Print_UserInputToScreen
c$ACE6 PUSH AF       ; Stash the user input keypress on the stack.
 $ACE7 CP $0D        ; {Jump to #R$ACF3 if "DELETE" was not pressed.
 $ACE9 JR NZ,$ACF3   ; }
 $ACEB PUSH HL       ; Stash the pointer to the command buffer on the stack.
; Print "SPACE<BS>" to delete the letter from the screen.
 $ACEC LD HL,$ACC9   ; #REGhl=#R$ACC9.
 $ACEF CALL $A585    ; Call #R$A585.
 $ACF2 POP HL        ; Restore the pointer to the command buffer from the
                     ; stack.
@label=UserInputToScreen
*$ACF3 POP AF        ; Restore the user input keypress from the stack.
 $ACF4 CALL $A577    ; Call #R$A577.
; Print the user input keypress to the screen.
 $ACF7 CALL $A5A4    ; Call #R$A5A4.
 $ACFA RET           ; Return.

; Handler: Match Item
;
; Used by the routine at #R$AD32.
;
; O:F The carry flag is set to indicate that an item was found
@label=Handler_MatchItem
c$ACFB PUSH HL       ; {Stash #REGhl, #REGde, #REGbc and #REGix on the stack.
 $ACFC PUSH DE       ;
 $ACFD PUSH BC       ;
 $ACFE PUSH IX       ; }
 $AD00 LD HL,($A7C6) ; #REGhl=*#R$A7C6.
 $AD03 LD C,$00      ; #REGc=#N$00.
 $AD05 JR $AD14      ; Jump to #R$AD14.
*$AD07 LD E,B        ; #REGe=#REGb.
 $AD08 LD D,$00      ; #REGd=#N$00.
 $AD0A ADD HL,DE     ; #REGhl+=#REGde.
 $AD0B LD A,(HL)     ; #REGa=*#REGhl.
 $AD0C CP $2C        ; Compare #REGa with #N$2C.
 $AD0E JR Z,$AD13    ; Jump to #R$AD13 if #REGa is equal to #N$2C.
 $AD10 INC C         ; Increment #REGc by one.
 $AD11 JR $AD14      ; Jump to #R$AD14.
*$AD13 INC HL        ; Increment #REGhl by one.
*$AD14 LD A,(HL)     ; {Jump to #R$AD2C if *#REGhl is equal to #N$FF.
 $AD15 CP $FF        ;
 $AD17 JR Z,$AD2C    ; }
 $AD19 LD IX,$A82F   ; #REGix=#R$A82F.
 $AD1D LD B,$04      ; #REGb=#N$04.
*$AD1F LD A,(IX+$00) ; {Jump to #R$AD07 if *#REGix+#N$00 is not equal to
 $AD22 CP (HL)       ; *#REGhl.
 $AD23 JR NZ,$AD07   ; }
 $AD25 INC HL        ; Increment #REGhl by one.
 $AD26 INC IX        ; Increment #REGix by one.
 $AD28 DJNZ $AD1F    ; Decrease counter by one and loop back to #R$AD1F until
                     ; counter is zero.
 $AD2A LD A,C        ; #REGa=#REGc.
 $AD2B SCF           ; Set the carry flag to indicate that an item was
                     ; successfully found.
@label=MatchItem_Return
*$AD2C POP IX        ; {Restore #REGix, #REGbc, #REGde and #REGhl from the
 $AD2E POP BC        ; stack.
 $AD2F POP DE        ;
 $AD30 POP HL        ; }
 $AD31 RET           ; Return.

; Handler: User Input
;
; Used by the routine at #R$E99F.
; .
; Handles keyboard input, tokenises commands and validates the
; vocabulary.
;
; .
;
; Reset the screen position to defaults.
@label=Handler_UserInput
c$AD32 CALL $A560    ; Call #R$A560.
 $AD35 CALL $A647    ; Call #R$A647.
; Initialise the command buffer.
 $AD38 LD HL,$A7F2   ; #REGhl=#R$A7F2.
 $AD3B LD B,$00      ; Initialise a letter counter in #REGb.
 $AD3D JR $AD41      ; Jump to #R$AD41.
; Main input loop - process each keypress.
@label=UserInput_Loop
*$AD3F INC HL        ; Move to the next byte of the command buffer.
 $AD40 INC B         ; Increment the letter counter by one.
@label=UserInput_Next
*$AD41 CALL $ACCF    ; Call #R$ACCF.
 $AD44 CALL $A53E    ; Call #R$A53E.
 $AD47 CP $0C        ; {Jump to #R$AD5B if "DELETE" was not pressed.
 $AD49 JR NZ,$AD5B   ; }
; The user pressed "DELETE".
 $AD4B XOR A         ; {Jump back to #R$AD41 if there hasn't been any input yet (nothing to
 $AD4C OR B          ; delete).
 $AD4D JR Z,$AD41    ; }
; There is input which can be deleted, so action a delete!
 $AD4F EX DE,HL      ; Temporarily stash the command buffer pointer in #REGde.
; Print "SPACE BACKSPACE BACKSPACE SPACE BACKSPACE" to move the current
; print position on the screen to the previous character, and to delete the
; character present using a space.
 $AD50 LD HL,$ACC6   ; #REGhl=#R$ACC6.
 $AD53 CALL $A585    ; Call #R$A585.
; Adjust the command buffer position and letter counter.
 $AD56 EX DE,HL      ; Restore the command buffer pointer from #REGde.
 $AD57 DEC HL        ; Decrease the command buffer pointer by one.
 $AD58 DEC B         ; Decrease the letter counter by one.
 $AD59 JR $AD41      ; Jump to #R$AD41.
; Check which key the user pressed:
@label=ValidateUserInput
*$AD5B LD C,A        ; {Jump to #R$AD6D if "ENTER" was pressed.
 $AD5C CP $0D        ;
 $AD5E JR Z,$AD6D    ; }
 $AD60 CP $20        ; {If the keypress was any other control key (the value being under
 $AD62 JR C,$AD41    ; #N$20 ASCII "SPACE"), it's not valid input so jump back to #R$AD41.
                     ; }
 $AD64 CP $80        ; {If the keypress was higher than #N$80, it's also not valid input so
 $AD66 JR NC,$AD41   ; jump back to #R$AD41.}
; Is the command buffer full?
 $AD68 LD A,B        ; {Jump to #R$AD41 if the letter counter is #N$31 (so the buffer is
 $AD69 CP $31        ; full).
 $AD6B JR Z,$AD41    ; }
; Writes the keypress into the command buffer and print it to the screen.
@label=UserInput_WriteKeypress
*$AD6D LD A,C        ; {Write the user input key to *#REGhl.
 $AD6E LD (HL),A     ; }
 $AD6F CALL $ACE6    ; Call #R$ACE6.
; Did the user press "ENTER"?
 $AD72 LD A,C        ; {Jump to #R$AD3F if "ENTER" was not pressed.
 $AD73 CP $0D        ;
 $AD75 JR NZ,$AD3F   ; }
; The player pressed "ENTER" so begin to process the user input.
; .
; Clear down the user input tokens.
 $AD77 LD HL,$A824   ; #REGhl=#R$A824.
 $AD7A LD B,$0A      ; Set a counter in #REGb for all #N$0A user input tokens.
@label=EmptyUserInputTokens_Loop
*$AD7C LD (HL),$FF   ; Write a termination byte (#N$FF) to *#REGhl.
 $AD7E INC HL        ; Increment #REGhl by one.
 $AD7F DJNZ $AD7C    ; Decrease the user input tokens counter by one and loop back to
                     ; #R$AD7C until all the tokens have been set to termination bytes (#N$FF).
; Set up pointers for the command buffer, the user input tokens and the
; count of the number of user input tokens.
 $AD81 LD HL,$A7F2   ; #REGhl=#R$A7F2.
 $AD84 LD IX,$A824   ; #REGix=#R$A824.
 $AD88 LD C,$0A      ; Set a counter in #REGc for the #N$0A user input tokens.
 $AD8A JR $ADF6      ; Jump to #R$ADF6.
; What's been entered isn't parsable.
; .
; Print "#STR$A84F,$08($b==$FF)".
@label=Response_NotUnderstood
*$AD8C LD HL,$A84F   ; #REGhl=#R$A84F.
 $AD8F CALL $A592    ; Call #R$A592.
 $AD92 JP $AD32      ; Jump to #R$AD32.
; Process found word into user input token.
@label=ProcessFoundWord
*$AD95 PUSH HL       ; {Stash the command buffer pointer, #REGde and user input tokens
 $AD96 PUSH DE       ; counter on the stack.
 $AD97 PUSH BC       ; }
 $AD98 LD HL,($ACC2) ; #REGhl=*#R$ACC2.
 $AD9B LD DE,$A82F   ; #REGde=#R$A82F.
; Ensure that the length is not more than #N$04.
 $AD9E LD BC,$0004   ; #REGbc=#N($0004,$04,$04).
 $ADA1 LD A,($ACC4)  ; {Jump to #R$ADA8 if *#R$ACC4 is greater than or equal to
 $ADA4 CP C          ; #N$04.
 $ADA5 JR NC,$ADA8   ; }
; The length is less than #N$04, so no need to copy #N$04 bytes.
 $ADA7 LD C,A        ; Copy the actual length of the word into #REGc.
; Copy the word (up to #N$04 characters in length) into the four letter
; buffer.
@label=CopyWordToBuffer
*$ADA8 LDIR          ; Copy the "up-to-#N$04" letters from the command buffer into the
                     ; four-letter buffer.
 $ADAA POP BC        ; {Restore the user input token counter, #REGde and command buffer
 $ADAB POP DE        ; pointer from the stack.
 $ADAC POP HL        ; }
 $ADAD PUSH HL       ; Stash the command buffer pointer back on the stack.
; Stash the vocabulary pointer for a separate check...
 $ADAE LD HL,($A7C6) ; {Stash *#R$A7C6 on the stack.
 $ADB1 PUSH HL       ; }
; Ignore any usage of "THE" - this is a very good idea! If the player
; enters: "EXAMINE THE SKULL", this ensures it's evaluated in the exact same
; way as "EXAMINE SKULL" (or really, "EXAM SKUL").
 $ADB2 LD HL,$A667   ; {Write #R$A667 to *#R$A7C6.
 $ADB5 LD ($A7C6),HL ; }
 $ADB8 CALL $ACFB    ; Call #R$ACFB.
; Do nothing with with this match - just restore the vocabulary pointer
; from the stack.
 $ADBB POP HL        ; {Restore *#R$A7C6 from the stack and write it back to
 $ADBC LD ($A7C6),HL ; *#R$A7C6.}
 $ADBF POP HL        ; Restore #REGhl from the stack.
 $ADC0 JR C,$ADF6    ; Jump to #R$ADF6 if "THE" was successfully found.
 $ADC2 CALL $ACFB    ; Call #R$ACFB.
 $ADC5 JR C,$ADEE    ; Jump to #R$ADEE if an item was successfully found.
; Nothing matched ... Inform the player.
; .
; Print "#STR$A863,$08($b==$FF)".
 $ADC7 LD HL,$A863   ; #REGhl=#R$A863.
 $ADCA CALL $A592    ; Call #R$A592.
; #HTML(Print a double quote character: "<code>#CHR$22</code>".)
 $ADCD LD A,$22      ; {Call #R$A5A4 to print a double quote character (ASCII
 $ADCF CALL $A5A4    ; #N$22).}
 $ADD2 LD HL,($ACC4) ; #REGhl=*#R$ACC4.
 $ADD5 LD DE,($ACC2) ; {#REGhl+=*#R$ACC2.
 $ADD9 ADD HL,DE     ; }
 $ADDA LD (HL),$FF   ; Write #N$FF to *#REGhl.
 $ADDC EX DE,HL      ; Exchange the #REGde and #REGhl registers.
 $ADDD CALL $A585    ; Call #R$A585.
; #HTML(Print a double quote character: "<code>#CHR$22</code>".)
 $ADE0 LD A,$22      ; {Call #R$A5A4 to print another double quote character
 $ADE2 CALL $A5A4    ; (ASCII #N$22).}
; Print "#STR$AA15,$08($b==$FF)".
 $ADE5 LD HL,$AA15   ; #REGhl=#R$AA15.
 $ADE8 CALL $A592    ; Call #R$A592.
 $ADEB JP $AD32      ; Jump to #R$AD32.
; The word in the four-letter buffer was matched!
; .
; Store the found token and check if we can continue parsing.
@label=StoreTokenAndContinue
*$ADEE LD (IX+$00),A ; Write the token to the current token slot.
 $ADF1 INC IX        ; Move to the next token slot.
 $ADF3 DEC C         ; Decrease the token counter by one.
 $ADF4 JR Z,$AE2D    ; Jump to #R$AE2D if all token slots are filled.
; Token matching only uses four letters of every word so a buffer is used
; for processing.
; .
; Start by clearing the buffer.
@label=EmptyFourLetterBuffer
*$ADF6 PUSH HL       ; {Stash the command buffer pointer and user input tokens counter on
 $ADF7 PUSH BC       ; the stack.}
 $ADF8 LD HL,$A82F   ; Load #R$A82F into #REGhl.
 $ADFB LD B,$04      ; Set a counter in #REGb for the #N$04 letters in the
                     ; buffer.
@label=EmptyFourLetterBuffer_Loop
*$ADFD LD (HL),$20   ; Write ASCII "SPACE" (#N$20) to *#REGhl.
 $ADFF INC HL        ; Increment #REGhl by one.
 $AE00 DJNZ $ADFD    ; Decrease the letter buffer counter by one and loop back to #R$ADFD
                     ; until all four letters have been cleared.
 $AE02 POP BC        ; {Restore the user input tokens counter and command buffer pointer
 $AE03 POP HL        ; from the stack.}
 $AE04 PUSH DE       ; Stash #REGde on the stack.
 $AE05 JR $AE08      ; Jump to #R$AE08.
; Skip delimiter characters to find the next word.
@label=SkipDelimiters
*$AE07 INC HL        ; Move to the next byte of the command buffer.
@label=FindWordStart
*$AE08 LD A,(HL)     ; Fetch a character from the command buffer.
 $AE09 CP $0D        ; {Jump to #R$AE29 if the character is "ENTER" (ASCII
 $AE0B JR Z,$AE29    ; #N$0D).}
 $AE0D CALL $ACD8    ; Call #R$ACD8.
 $AE10 JR Z,$AE07    ; Jump to #R$AE07 if the character is a delimiter.
; The character is not a delimiter.
 $AE12 LD ($ACC2),HL ; Store the starting address of this new word in *#R$ACC2.
 $AE15 LD DE,$0000   ; Initialise #REGde to #N($0000,$04,$04) to count the number of
                     ; letters in the word.
; Just keep looping and moving across the command buffer until we hit a
; delimiter or an "ENTER" character.
@label=CountWordLength
*$AE18 INC HL        ; Move to the next byte of the command buffer.
 $AE19 INC DE        ; Increment the letter counter by one.
 $AE1A LD A,(HL)     ; Fetch a character from the command buffer.
 $AE1B CP $0D        ; {Jump to #R$AE24 if the character is "ENTER" (ASCII
 $AE1D JR Z,$AE24    ; #N$0D).}
 $AE1F CALL $ACD8    ; Call #R$ACD8.
 $AE22 JR NZ,$AE18   ; Jump to #R$AE18 if the character was not a delimiter.
; This is the end of the word, so store the word length.
@label=StoreWordLength
*$AE24 LD ($ACC4),DE ; Write the letter counter to *#R$ACC4.
 $AE28 SCF           ; Set the carry flag to indicate that a word was
                     ; successfully found.
@label=EndWordParsing
*$AE29 POP DE        ; Restore #REGde from the stack.
 $AE2A JP C,$AD95    ; Jump to #R$AD95 if a word was found.
; Check to see if any of the user input tokens have been populated, and
; if not, print "#STR$A84F,$08($b==$FF)".
@label=CheckVerbToken
*$AE2D LD A,($A824)  ; {Jump to #R$AD8C if *#R$A824 hold the terminator byte
 $AE30 CP $FF        ; (#N$FF).
 $AE32 JP Z,$AD8C    ; }
 $AE35 RET           ; Return.
; View the equivalent code in #JEWELS$C00A.

; Parser: Validate Item
;
; Used by the routine at #R$AFC7.
;
;   BC The position in the user input token of the object
; O:F The carry flag is set when the item isn't present
@label=Parser_ValidateItem
c$AE36 INC BC        ; Adjust the counter for the sum.
 $AE37 LD A,($A7E8)  ; {Calculate the index (*#R$A7E8-#REGc) and store the
 $AE3A SUB C         ; result in #REGe.
 $AE3B LD E,A        ; }
 $AE3C PUSH DE       ; Stash #REGde on the stack.
 $AE3D LD IX,($A7D4) ; {Call #R$AB88 with *#R$A7D4.
 $AE41 CALL $AB88    ; }
 $AE44 POP DE        ; Restore #REGde from the stack.
 $AE45 JR $AE48      ; Jump to #R$AE48.
@label=ValidateItem_Loop
*$AE47 INC HL        ; Increment #REGhl by one.
@label=ValidateItem
*$AE48 LD A,(HL)     ; {Jump to #R$AE53 if *#REGhl is equal to #N$FF.
 $AE49 CP $FF        ;
 $AE4B JR Z,$AE53    ; }
 $AE4D CALL $AE6B    ; Call #R$AE6B.
 $AE50 JR NZ,$AE47   ; Jump to #R$AE47 if the item wasn't found.
 $AE52 RET           ; Return.
; The item being referenced isn't in the room or in the players inventory.
; .
; Print "#STR$A8DB,$08($b==$FF)".
@label=Response_ItemNotHere
*$AE53 LD HL,$A8DB   ; #REGhl=#R$A8DB.
 $AE56 CALL $A585    ; Call #R$A585.
; Print the object name.
 $AE59 LD IX,($A7D6) ; {Call #R$AB88 with *#R$A7D6.
 $AE5D CALL $AB88    ; }
 $AE60 CALL $A585    ; Call #R$A585.
; Print "#STR$A8E8,$08($b==$FF)".
 $AE63 LD HL,$A8E8   ; #REGhl=#R$A8E8.
 $AE66 CALL $A592    ; Call #R$A592.
 $AE69 SCF           ; Set the carry flag.
 $AE6A RET           ; Return.
